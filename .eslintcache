,"60","61","62"],"import React,{useState, FormEvent} from 'react';\nimport { useHistory } from 'react-router-dom';\n\nimport Headerbar from '../components/HeaderBar';\nimport TitleItem from '../components/TitleItem';\n\nimport '../styles/pages/register.css';\n\n\nexport default function Register(){\n    const history = useHistory();\n    const [tableId, setTableId] = useState(Number(localStorage.getItem('@table/type')));\n    const[name, setName] = useState<string>('');\n    const[card_number, setCardNumber] = useState<string>('');\n    const[month_year, setMonthYear] = useState<string>('');\n    const[code, setCode] = useState<string>('');\n    const tagCard = '@card/data';\n\n\n    function handleSubmit(event: FormEvent){\n        event.preventDefault();\n        const data = {\n            'name': name,\n            'card_number': card_number,\n            'month_year': month_year,\n            'code': code\n        }\n        localStorage.setItem(tagCard, JSON.stringify(data));\n        history.push(`/confirmation/${tableId}`);\n    };\n\n    return(\n        <>\n            <Headerbar/>\n            <div className=\"container\">\n                <TitleItem text=\"Solicitar Empréstimo\"/>\n                <form onSubmit={handleSubmit}>\n                    <div className=\"all-info-card\">\n                        <div className=\"card-left-data\">\n                            <label className=\"title\" htmlFor=\"left-lable\">Insira os dados do Cartão</label>\n                            <div className=\"card-data\">                \n                                <input \n                                    id=\"name\"\n                                    required={true}\n                                    placeholder=\"Nome\" \n                                    value={name} \n                                    onChange={event => setName(event.target.value)}\n                                />\n                            </div>\n                            <div className=\"card card-data\">                \n                                <input \n                                    id=\"card-number\"\n                                    required={true}\n                                    placeholder=\"Numero do cartão\" \n                                    value={card_number} \n                                    onChange={event => setCardNumber(event.target.value)}\n                                />\n                            </div>\n                            <div className=\"card-data\">                \n                                <input \n                                    id=\"month_year\"\n                                    required={true}\n                                    placeholder=\"Data de Validade\" \n                                    value={month_year} \n                                    onChange={event => setMonthYear(event.target.value)}\n                                />\n                            </div>\n                            <div className=\"card-data\">                \n                                <input \n                                    id=\"code\"\n                                    required={true}\n                                    placeholder=\"CVC\" \n                                    value={code} \n                                    onChange={event => setCode(event.target.value)}\n                                />\n                            </div>\n                        </div>\n                        <div className=\"card-right-data\">\n                            <label className=\"title\" htmlFor=\"left-lable\">Insira os dados do Cartão</label>\n                            <div className=\"card-attachment\">\n                                <label htmlFor=\"images\">Cartão de Crédito (Frente)</label>\n                                <a href=\"#\">Adicionar</a>\n                            </div>\n                            <div className=\"card-attachment\">\n                                <label htmlFor=\"images\">Cartão de Crédito (Verso)</label>\n                                <a href=\"#\">Adicionar</a>\n                            </div>\n                            <div className=\"card-attachment\">\n                                <label htmlFor=\"images\">Selfie com cartão de crédito</label>\n                                <a href=\"#\">Adicionar</a>\n                            </div>\n                            <label className=\"attention\" htmlFor=\"attention\">Atenção: As fotos devem estar legíveis, com todas as informações visíveis do cartão.</label>\n                        </div>\n                    </div>\n                    <button className=\"foward-button\" type=\"submit\">\n                    Confirmar\n                    </button>\n                </form>\n            </div>\n        </>    \n    )\n}","/home/whalyf/Documentos/projects/klutch/frontend/src/components/HeaderBar.tsx",[],"/home/whalyf/Documentos/projects/klutch/frontend/src/index.tsx",[],["63","64"],"/home/whalyf/Documentos/projects/klutch/frontend/src/pages/CardType.tsx",[],"/home/whalyf/Documentos/projects/klutch/frontend/src/pages/Confirmation.tsx",["65","66","67","68"],"/home/whalyf/Documentos/projects/klutch/frontend/src/pages/ConfirmationSucceed.tsx",["69","70","71","72"],"/home/whalyf/Documentos/projects/klutch/frontend/src/pages/Details.tsx",["73","74","75","76","77","78","79","80"],"/home/whalyf/Documentos/projects/klutch/frontend/src/services/api.ts",[],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":12,"column":21,"nodeType":"87","messageId":"88","endLine":12,"endColumn":31},{"ruleId":"89","severity":1,"message":"90","line":82,"column":33,"nodeType":"91","endLine":82,"endColumn":45},{"ruleId":"89","severity":1,"message":"90","line":86,"column":33,"nodeType":"91","endLine":86,"endColumn":45},{"ruleId":"89","severity":1,"message"